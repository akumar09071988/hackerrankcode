import java.util.Scanner;


public class Solution {

	public static void main(String[] args) {
		
		try
		{
			Scanner in = new Scanner(System.in);
			int row = in.nextInt();
			int col = in.nextInt();
			int startNum = -1;
			int i =-1;
			if(row%2!=0)
			{
				startNum =0;
				i=1;
			}
			else
			{
				startNum =1;
				i=2;
			}
			while(i<=row)
			{
				startNum = startNum +10;
				i+=2;
			}
			int j=1;
			while(j<col)
			{
				startNum+=2;
				j++;
			}
		}
		catch(Exception e)
		{
			e.printStackTrace();
		}

	}

}
